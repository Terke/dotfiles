  <script type="text/javascript">
    ghostery_enabled = true;
    var status_str = {
      'yes'    : "<span style='color: green; font-size: 15pt;'>&#x2713;</span><span class='complimentary_text'>&nbsp;yes</span>",
      'no'     : "<span style='color: red; font-size: 15pt;'>&#x2717;</span><span class='complimentary_text'>&nbsp;no</span>",
      'partial': "<span style='color: orange; font-size: 15pt;'>&#x26A0;</span><span class='complimentary_text'>&nbsp;partial protection</span>"
    };
    var fp_status_str = {
      'yes'      : "<span style='color: green; font-size: 15pt;'>&#x2713;</span><span class='complimentary_text'>&nbsp;your browser has a non-unique fingerprint</span>",
      'no'       : "<span style='color: red; font-size: 15pt;'>&#x2717;</span><br><span class='complimentary_text'>your browser has a nearly-unique fingerprint",
      'no_unique': "<span style='color: red; font-size: 15pt;'>&#x2717;</span><br><span class='complimentary_text'>your browser has a unique fingerprint",
      'partial'  : "<span style='color: orange; font-size: 15pt;'>&#x26A0;</span><span class='complimentary_text'>&nbsp;partial protection</span>"
    };
    var download_links = "<div id='badger-download' javascript='disabled'><a href='https://www.eff.org/files/privacy-badger-latest.xpi' id='badger-firefox'>Download for Firefox</a> <br> <a href='https://chrome.google.com/webstore/detail/pkehgijcmpdhfbdbbnkijodmdjhbjlgp' id='badger-chrome'>Download for Chrome</a> <br> <a href='https://addons.opera.com/en/extensions/details/privacy-badger' id='badger-opera'>Download for Opera</a></div>";
    var fpi_whorls = JSON.parse('{"v2":{"audio":"35.10892750695348","canvas_hash_v2":"ec4bce9082998b1a9d515657a41c72b4","webgl_hash_v2":"13ae805231fcd00154a46b5a992143ec"}}');
    window.onload = function(){
      document.getElementById('fingerprintTable').style.display = "none";
      document.getElementById('buttons').style.display = "none";

      var a_loads = 0;
      var t_loads = 0;
      var dnt_loads = 0;
      var acceptable_ads_test = 1;

      if(acceptable_ads_test){
        $('#kcarterlink').attr('href', $('#kcarterlink').attr('href') + '?aat=1');
      }

      (function fingerprint_table_ux(){
        var showFingerprintLink1 = document.getElementById('showFingerprintLink1');
        var showFingerprintLink2 = document.getElementById('showFingerprintLink2');
        var fingerprintTable = document.getElementById('fingerprintTable')

        showFingerprintLink1.onclick = showFingerprintLink2.onclick = function(){
          showFingerprintLink2.style.display = 'none';
          fingerprintTable.style.display = 'block';
        };
      })();

      (function load_iframes(){
        var cb = Math.floor((Math.random()*1000000)+1);
        var a = document.getElementById('a');
        a.src = 'https://trackersimulator.org/kcarting-tally?&random=' + cb;

        var t = document.getElementById('t');
        t.src = 'https://eviltracker.net/kcarting-tally?&random=' + cb;

        var dnt = document.getElementById('dnt');
        dnt.src = 'https://do-not-tracker.org/kcarting-tally?&random=' + cb;

        var kcarterlink = document.getElementById('kcarterlink');
        kcarterlink.onclick = function clearCookies(){
          a.src = 'https://trackersimulator.org/clear-cookies?&random=' + cb;
          t.src = 'https://eviltracker.net/clear-cookies?&random=' + cb;
          dnt.src = 'https://do-not-tracker.org/clear-cookies?&random=' + cb;

          setTimeout(function(){
            window.location = kcarterlink.href;
          }, 2000);
          return false;
        }
      })();

      var results = {'known_blockers': []};
      function display_results(){
        var metrics = ['ad', 'tracker', 'dnt', 'acceptable_ads'];

        for(x in metrics){
          var metric = metrics[x];
          if(metric in results){
            document.getElementById(metric + '_status').innerHTML = status_str[results[metric]];
          }
        }
      }

      var counter = 0;
      var sites_responding = {};
      var tracker_assumed_status;
      var ad_assumed_status;
      (function handle_cross_origin_communication(){
        window.addEventListener("message", receiveMessage, false);

        function receiveMessage(event){
          counter++;
          sites_responding[event.origin] = true;
          if(event.origin == "https://trackersimulator.org"){
            a_cookies = event.data.split(" ").length;
            if(a_loads == 3){
              if(a_loads > a_cookies){
                ad_assumed_status = 'partial';
              } else {
                ad_assumed_status = 'no';
              }
            } else {
              results['ad'] = 'yes';
            }
          } else if(event.origin == "https://eviltracker.net"){
            t_cookies = event.data.split(" ").length;
            if(t_loads == 3){
              if(t_loads > t_cookies){
                tracker_assumed_status = 'partial';
              } else {
                tracker_assumed_status = 'no';
              }
            } else {
              results['tracker'] = 'yes';
            }
          }
          display_results();
        }
      })();

      function install_link(browser, url) {
        var install = document.createElement('a');
        install.href = url;
        install.innerHTML = "Install Privacy Badger<br><div>and Enable Do Not Track</div>";
        install.onclick = function(){
          if(browser == 'Chrome'){
            event.preventDefault();
            chrome.webstore.install(url, function(){}, function(){onChromeInstallFailure(url)});
          }
        }
        return install;
      }

      function other_link(browser, url){
        var other = document.createElement('p');
        other.className = 'other_link';
        other.href = url;
        other.innerHTML = '<a href="'+url+'">Click here for '+browser+' version</a>';
        return other;
      }

      function onChromeInstallFailure(url){
        window.location.assign(url);
      }

      function detect_browser_and_platform(){
        // detect browser
        var browser = 'other';
        var platform = 'desktop';
        if(navigator && navigator.userAgent){
          if(navigator.userAgent.match(/firefox/i)){
            browser = 'firefox';
            if(navigator.userAgent.match(/android/i)){
              platform = 'android';
            }
          } else if(navigator.userAgent.match(/msie/i) || navigator.userAgent.match(/trident/i)){
            browser = 'ie';
          } else if(navigator.userAgent.match(/opr\//i)){
            browser = 'opera';
            if(navigator.userAgent.match(/mobile/i)){
              platform = 'android';
            }
          } else if(navigator.userAgent.match(/chrome/i)) {
            browser = 'chrome';
            if(navigator.userAgent.match(/android/i)){
              platform = 'android';
            }
          } else if(navigator.userAgent.match(/fxios/i)){
            platform = 'ios';
            browser = 'firefox';
          } else if(navigator.userAgent.match(/crios/i)){
            platform = 'ios';
            browser = 'chrome';
          } else if(navigator.userAgent.match(/safari/i)){
            browser = 'safari';
            if(navigator.userAgent.match(/mobile/i)){
              platform = 'ios';
            }
          }
        }
        return {browser: browser, platform: platform};
      }

      // this function is mirrored by get_tool_recommendation in util.py.  if you change it here, make sure it's changed there as well.
      function prepare_install_button(detection){
        var firefox_url = document.querySelector('#badger-firefox').href;
        var chrome_url = document.querySelector('#badger-chrome').href;
        var opera_url = document.querySelector('#badger-opera').href;
        var badger_download = document.getElementById('badger-download');
        var tool_recommendation = document.getElementById('tool-recommendation');
        var platform = detection.platform;
        var browser = detection.browser;
        badger_download.innerHTML = "";
        badger_download.setAttribute('javascript', 'enabled');
        // firefox
        if(platform == 'desktop'){
          if(browser == 'firefox'){
            badger_download.onclick = function(){
              window.location = firefox_url;
            };
            badger_download.appendChild(install_link('Firefox', firefox_url));
            badger_download.parentNode.insertBefore(other_link('Opera', opera_url),badger_download.nextSibling);
            badger_download.parentNode.insertBefore(other_link('Chrome', chrome_url),badger_download.nextSibling);
          }
          // chrome
          else if(browser == 'chrome'){
            badger_download.onclick = function(){
              window.location = chrome_url;
            };
            badger_download.appendChild(install_link('Chrome', chrome_url));
            badger_download.parentNode.insertBefore(other_link('Opera', opera_url),badger_download.nextSibling);
            badger_download.parentNode.insertBefore(other_link('Firefox', firefox_url),badger_download.nextSibling);
          } else if(browser == 'opera') {
            badger_download.onclick = function(){
              window.location = opera_url;
            };
            badger_download.appendChild(install_link('Opera', opera_url));
            badger_download.parentNode.insertBefore(other_link('Chrome', chrome_url),badger_download.nextSibling);
            badger_download.parentNode.insertBefore(other_link('Firefox', firefox_url),badger_download.nextSibling);
          } else if(browser == 'ie'){
            badger_download.style.display = "none";
            tool_recommendation.href = "http://windows.microsoft.com/en-us/internet-explorer/use-tracking-protection#ie=ie-11";
            tool_recommendation.innerHTML = "adding tracker protection lists EasyList and EasyPrivacy";
          } else {
            badger_download.style.display = "none";
          }
        } else if(platform == 'ios') {
          if(browser == 'safari'){
            badger_download.style.display = "none";
            tool_recommendation.href = "https://itunes.apple.com/us/app/disconnect-best-blocking-award/id935480186?mt=8";
            tool_recommendation.innerHTML = "Disconnect";
          } else {
            badger_download.style.display = "none";
            tool_recommendation.href = "https://itunes.apple.com/us/app/disconnect-privacy-pro-entire/id1057771839?mt=8";
            tool_recommendation.innerHTML = "Disconnect Privacy Pro";
          }
        } else if(platform == 'android'){
          if(browser == 'firefox'){
            badger_download.style.display = "none";
            tool_recommendation.href = "https://addons.mozilla.org/en-US/android/addon/ublock-origin/";
            tool_recommendation.innerHTML = "uBlock Origin";
          } else {
            badger_download.style.display = "none";
            tool_recommendation.href = "https://disconnect.me/mobile/disconnect-mobile/sideload";
            tool_recommendation.innerHTML = "Disconnect";
          }
        }
      }

      function record_results(){
        var req = new XMLHttpRequest();
        req.open("POST", "/record-results",true);
        req.setRequestHeader('Content-type','application/json')
        req.send(JSON.stringify(results));
      }

      setTimeout(function(){
        if(counter < 3){
          // We're using PrivacyBadger (heuristic blocking) or domain blacklist
          results['ad'] = 'yes';
          results['tracker'] = 'yes';
          results['acceptable_ads'] = 'yes';
          if("https://do-not-tracker.org" in sites_responding){
            results['dnt'] = 'yes';
          } else {
            results['dnt'] = 'no';
          }
        } else {
          results['dnt'] = 'no';
          if(ghostery_enabled){
            results['tracker'] = 'yes';
            results['ad'] = 'yes';
            results['known_blockers'].push('ghostery');
          } else {
            if(tracker_assumed_status){
              results['tracker'] = tracker_assumed_status;
            }
            if(ad_assumed_status){
              results['ad'] = ad_assumed_status;
            }
          }
        }

        // Only display the results of the acceptable ads test if we have at
        // least *some* protection against ads or trackers.
        if(acceptable_ads_test && !(results['ad'] == "no" && results['tracker'] == "no")){
          // If we don't already have a 'yes' result from Privacy Badger...
          if(results['acceptable_ads'] != 'yes'){
            results['acceptable_ads'] = 'yes';

            var meta = document.createElement('meta');
            meta.name = "referrer";
            meta.content = "no-referrer";
            document.getElementsByTagName('head')[0].appendChild(meta);

            var acceptable_ad = document.createElement('img');
            acceptable_ad.onload = function(){
              results['acceptable_ads'] = 'no';
              if(results['tracker'] == 'yes'){
                results['tracker'] = 'partial';
              }
              if(results['ad'] == 'yes'){
                results['ad'] = 'partial';
              }
              display_results();
              display_summary_sentence();
            };
            acceptable_ad.src = "https://media.net/favicon.ico";
          }
          $('#acceptable_ads_tr').show();
        }

        display_results();
        // final string from results...

        function display_summary_sentence(){
          var summary_sentence = document.getElementById('summary_sentence');

          if(results['tracker'] == 'yes' && results['ad'] == 'yes' && results['dnt'] == 'yes'){
            summary_sentence.innerHTML = "Yes! You have <strong>strong protection against Web tracking</strong>.<p>Help us defend the Web against tracking:</p><div class='social'><a title='Share with Twitter' target='_blank' href='https://twitter.com/home?status=Worried%20about%20online%20tracking?%20Find%20out%20how%20vulnerable%20you%20really%20are%20with%20Panopticlick%20https%3A//panopticlick.eff.org&amp;related=eff' class='twitter'></a><a title='Share with Facebook' target='_blank' href='https://www.facebook.com/sharer/sharer.php?u=https%3A%2F%2Fpanopticlick.eff.org%2F&amp;display=popup' class='facebook'></a><a title='Share with Google+' target='_blank' href='https://plus.google.com/share?url=https://panopticlick.eff.org' class='googleplus'></a>    <a class='email no-margin' href='mailto:?subject=Panopticlick&amp;body=Find%20out%20how%20trackable%20your%20browser%20is%20at%20https%3A//panopticlick.eff.org' title='Share with Email'></a></div>";
          } else if(results['tracker'] == 'yes' && results['ad'] == 'yes' && results['dnt'] == 'no'){
            summary_sentence.innerHTML = "Yes! You have <strong>strong protection against Web tracking</strong>, though your software isn’t checking for Do Not Track policies.<p>Help us defend the Web against tracking:</p><div class='social'><a title='Share with Twitter' target='_blank' href='https://twitter.com/home?status=Worried%20about%20online%20tracking?%20Find%20out%20how%20vulnerable%20you%20really%20are%20with%20Panopticlick%20https%3A//panopticlick.eff.org&amp;related=eff' class='twitter'></a><a title='Share with Facebook' target='_blank' href='https://www.facebook.com/sharer/sharer.php?u=https%3A%2F%2Fpanopticlick.eff.org%2F&amp;display=popup' class='facebook'></a><a title='Share with Google+' target='_blank' href='https://plus.google.com/share?url=https://panopticlick.eff.org' class='googleplus'></a>    <a class='email no-margin' href='mailto:?subject=Panopticlick&amp;body=Find%20out%20how%20trackable%20your%20browser%20is%20at%20https%3A//panopticlick.eff.org' title='Share with Email'></a></div>";
          } else {
            if(results['tracker'] == 'no' && results['ad'] == 'no') {
              summary_sentence.innerHTML = "No. You are not protected against tracking on the Web. We suggest";
            } else if(ghostery_detected) {
              summary_sentence.innerHTML = "No. You seem to have some privacy software installed, but it <strong>isn't passing these tests</strong>. Consider";
            } else {
              summary_sentence.innerHTML = "Mixed results: you have <strong>some protection</strong> against Web tracking, but it has <strong>some gaps</strong>. We suggest <a href='https://www.eff.org/deeplinks/2017/11/how-debug-your-content-blocker-privacy-protection'>re-configuring</a> your protection software, or consider";
            }
            detection = detect_browser_and_platform();

            if(detection.platform == 'desktop' && (detection.browser == "chrome" || detection.browser == "firefox" || detection.browser == "opera")){
              summary_sentence.innerHTML += " <strong>installing EFF's Privacy Badger</strong>.";
            } else if(detection.platform == 'desktop' && detection.browser != "ie") {
              summary_sentence.innerHTML += " switching to a browser or OS that offers better protections.";
            } else {
              summary_sentence.innerHTML += " <strong>installing extra protections</strong>.  Privacy Badger isn't available for your browser / OS, but <a id='tool-recommendation' target='_blank'></a> may work for you.";
            }
            summary_sentence.innerHTML += download_links;

            prepare_install_button(detection);
          }
          document.getElementById('buttons').style.display = 'block';
        }
        display_summary_sentence();
        record_results();
      },2000);

      var tries = 0;
      ghostery_detected = null;
      var int_id = window.setInterval(function(){
        var purple_box  = document.querySelector('div[title="Click to dismiss alert bubble"]');

        //Ghostery installed
        if (purple_box != null){
          window.clearInterval(int_id);

          ghostery_detected = 'yes';
          /*var piwik_span = purple_box.querySelector('span');
          if(piwik_span.className.match(/blocked$/)){
            ghostery_detected = 'yes';
          } else {
            ghostery_detected = 'no';
          }*/
        } else if(tries >= 200){
          window.clearInterval(int_id);
        }
        tries += 1;
      },10);
    };
    window.onunload=function() {};
  </script>
